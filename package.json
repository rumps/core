{
  "name": "rump",
  "version": "0.8.3",
  "author": "Juan Placencia",
  "description": "Opinionated flow for assembling client assets",
  "license": "MIT",
  "homepage": "https://github.com/rumps/core",
  "main": "./lib",
  "jsnext:main": "./src",
  "repository": "rumps/core",
  "keywords": [
    "gulp",
    "rump"
  ],
  "scripts": {
    "codecov": "npm run coverage",
    "postcodecov": "codecov < coverage/coverage-final.json",
    "precompile": "[ -d lib ] && trash lib || true",
    "compile": "babel src --out-dir lib --source-maps=inline",
    "coverage": "nyc --reporter=json --reporter=html --reporter=text npm test",
    "lint": "eslint src test",
    "pub": "npm run test && npm run compile && npm publish",
    "pretest": "npm run lint",
    "test": "ava"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "chalk": "^1.1.0",
    "del": "^2.0.1",
    "extend": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.7.0",
    "babel": "^5.8.19",
    "babel-eslint": "^4.1.6",
    "bluebird": "^3.0.6",
    "codecov.io": "^0.1.5",
    "eslint": "^1.0.0",
    "eslint-config-jupl": "^0.2.1",
    "eslint-plugin-babel": "^3.0.0",
    "graceful-fs": "^4.1.2",
    "gulp": "^3.9.0",
    "mz": "^2.0.0",
    "nyc": "^4.0.1",
    "rump-a": "file:./test/fixtures/rump-a",
    "rump-b": "file:./test/fixtures/rump-b",
    "rumpc": "file:./test/fixtures/rumpc",
    "should": "^7.0.2",
    "should-sinon": "0.0.3",
    "sinon": "^1.15.4",
    "timeout-then": "^1.0.0",
    "trash": "^3.4.0"
  }
}
